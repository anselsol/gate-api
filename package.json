  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.4",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jasmine": "^3.6.3",
    "@types/lru-cache": "^5.1.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.14.28",
    "@types/swagger-ui-express": "^4.1.3",
    "chai": "^4.3.7",
    "concurrently": "^6.4.0",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "8.10.2",
    "ts-node-dev": "^1.1.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.1.4"
  },
  "dependencies": {
    "@metaplex/js": "4.12.0",
    "@prisma/client": "^4.8.1",
    "@solana/spl-token": "^0.3.6",
    "@solana/web3.js": "^1.37.1",
