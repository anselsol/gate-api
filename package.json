    "@types/request-promise": "^4.1.48",
    "@types/swagger-jsdoc": "^6.0.1",
    "axios": "^0.21.1",
    "bs58": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.1",
    "jose": "^4.11.1",
    "js-base64": "^3.7.2",
    "prisma": "^4.8.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "swagger-jsdoc": "^6.2.7",
    "swagger-ui-express": "^4.1.6"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --files src/index.ts",
    "clean": "rm -rf dist",
    "prepare": "run-s clean postinstall",
    "postinstall": "tsc && tsc-alias && npx prisma migrate reset --skip-generate --skip-seed -f && yarn db-migrate",
    "db-migrate": "npx prisma migrate deploy",
    "shell": "node -e \"$(< shell)\" -i --experimental-repl-await",
    "test": "mocha -r ts-node/register src/test/**/*.test.ts --timeout 0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}